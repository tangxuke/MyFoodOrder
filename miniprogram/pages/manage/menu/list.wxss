/* miniprogram/pages/manage/menu/list.wxss */
.fix-title .van-cell__title{
  min-width:90px !important;
  max-width:90px !important;
}

.item .van-cell__title{
  width: 100%;
  display: flex;
  flex-direction: row;
}

.item .van-cell__title .image{
  width:25%;
}

.van-swipe-cell__left,
.van-swipe-cell__right {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #f44;
}

.item{
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid rgba(0, 0, 0, 0.33);
}

.item-image{
  padding: 10px;
}

.item-content{
  flex-grow: 1;
}